@model IEnumerable<Imobiliare.Entities.BlockedIp>

@Html.Partial("_MenuSuperAdmin")

@{
    ViewBag.Title = "Index";
}

<h2>Dupa editare, reporniti aplicatia(edit web.config)</h2>

<p>
  <a href="@Url.Action("Create")" class="btn btn-success"><span class="glyphicon glyphicon-plus-sign"></span>&nbsp;Adauga</a>
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Address)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DataAdaugare)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.BlockCount)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Descriere)
        </th>
        <th>
            Trace log4net
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UpdateBlockCount)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IsRegex)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Enabled)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Address)
        </td>
        <td>
            @item.DataAdaugare.ToShortDateString()
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.BlockCount)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Descriere)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TraceBlocking)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UpdateBlockCount)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IsRegex)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Enabled)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
            @Html.ActionLink("Details", "Details", new { id = item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.Id })
        </td>
    </tr>
}

</table>
